{
  "app": "npx ts-node --prefer-ts-exts bin/trainium-spot.ts",
  "watch": {
    "include": ["**"],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json",
      "yarn.lock",
      "node_modules",
      "test"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target-partitions": ["aws", "aws-cn"],
    "availability-zones:account=600627330911:region=us-east-2": [
      "us-east-2a",
      "us-east-2b",
      "us-east-2c"
    ]
  }
}
